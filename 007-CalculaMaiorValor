//Mostra qual o maior valor

#include <stdio.h>
#include <locale.h>

int main(){
    setlocale(LC_ALL, "Portuguese");
    float A, B, C;
    printf("Digite 3 valores: ");
    scanf("%f %f %f", &A, &B, &C);
    if (A>B && A>C) printf("%.2f e o maior valor", A);
        else if(B>C) printf("%.2f e o maior valor", B);
            else printf("%.2f e o maior valor", C);
    return 0;
}
